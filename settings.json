{
  "GitCommitPlugin.MaxSubjectWords": 64,
  "[csharp]": {
    "editor.defaultFormatter": "ms-dotnettools.csharp"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[jade]": {
    "editor.defaultFormatter": "ducfilan.pug-formatter"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[less]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[sql]": {
    "editor.defaultFormatter": "adpyke.vscode-sql-formatter"
  },
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[xml]": {
    "editor.defaultFormatter": "DotJoshJohnson.xml"
  },
  "bookmarks.useWorkaroundForFormatters": true,
  "cSpell.caseSensitive": true,
  "cSpell.enableFiletypes": [
    "!json"
  ],
  "cSpell.language": "en,en-US",
  "cSpell.minWordLength": 4,
  "cSpell.spellCheckOnlyWorkspaceFiles": true,
  "cSpell.suggestionNumChanges": 3,
  "cSpell.useGitignore": true,
  "cSpell.userWords": [],
  "debug.allowBreakpointsEverywhere": true,
  "debug.console.closeOnEnd": true,
  "debug.openExplorerOnEnd": true,
  "diffEditor.codeLens": true,
  "diffEditor.maxComputationTime": 0,
  "editor.accessibilityPageSize": 50,
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.organizeImports": true
  },
  "editor.codeLensFontFamily": "'Jetbrains Mono', Consolas",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.foldingImportsByDefault": true,
  "editor.fontFamily": "'Jetbrains Mono', Consolas",
  "editor.fontLigatures": true,
  "editor.fontSize": 20,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.lineNumbers": "on",
  "editor.linkedEditing": true,
  "editor.maxTokenizationLineLength": 10000,
  "editor.minimap.size": "fill",
  "editor.mouseWheelZoom": true,
  "editor.renderControlCharacters": true,
  "editor.renderLineHighlightOnlyWhenFocus": true,
  "editor.rulers": [
    256
  ],
  "editor.scrollbar.verticalScrollbarSize": 5,
  "editor.smoothScrolling": true,
  "editor.suggest.shareSuggestSelections": true,
  "editor.suggest.showStatusBar": true,
  "editor.suggestSelection": "first",
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 80,
  "editor.wrappingIndent": "indent",
  "emmet.includeLanguages": {
    "javascript": "javascriptrect"
  },
  "errorLens.excludeBySource": [
    "cSpell"
  ],
  "errorLens.fontSize": "70%",
  "errorLens.gutterIconSet": "defaultOutline",
  "errorLens.gutterIconSize": "70%",
  "errorLens.gutterIconsEnabled": true,
  "errorLens.margin": "16%",
  "errorLens.messageMaxChars": 64,
  "errorLens.messageTemplate": "$message",
  "errorLens.padding": "16px",
  "errorLens.scrollbarHackEnabled": true,
  "errorLens.severityText": [
    "ERROR",
    "WARNING",
    "INFO",
    "HINT"
  ],
  "errorLens.statusBarColorsEnabled": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.associations": {
    "*.js": "javascript",
    "*.jsx": "javascriptreact",
    "*.ts": "typescript",
    "*.tsx": "typescriptreact",
    "*.vue": "vue",
    "*.wpy": "vue",
    "*.wxml": "html",
    "*.wxss": "css"
  },
  "files.autoGuessEncoding": true,
  "files.autoSave": "onFocusChange",
  "files.exclude": {
    "**/*.js": {
      "when": "$(basename).ts"
    },
    "**/*.meta": true,
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/.vs": true,
    "**/.vscode": true,
    "**/CVS": true,
    "**/node_modules": true,
    "**/var": true,
    "**/vendor": true,
    "library/": true,
    "local/": true,
    "temp/": true
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.ignoreLegacyWarning": true,
  "git.suggestSmartCommit": true,
  "gitlens.advanced.messages": {
    "suppressGitMissingWarning": true
  },
  "gitlens.codeLens.includeSingleLineSymbols": true,
  "guides.active.color.dark": "rgba(102,204,255,0.75)",
  "indentRainbow.colors": [
    "rgba(255,0,0,0.05)",
    "rgba(255,165,0,0.05)",
    "rgba(255,255,0,0.05)",
    "rgba(0,255,0,0.05)",
    "rgba(0,255,255,0.05)",
    "rgba(0,0,255,0.05)",
    "rgba(255,0,255,0.05)"
  ],
  "javascript.validate.enable": false,
  "less.compile": {
    "out": "${workspaceRoot}\\assets\\styles\\CSS\\"
  },
  "liveServer.settings.donotShowInfoMsg": true,
  "npm.runSilent": true,
  "omnisharp.disableMSBuildDiagnosticWarning": true,
  "omnisharp.organizeImportsOnFormat": true,
  "peacock.affectDebuggingStatusBar": true,
  "peacock.affectEditorGroupBorder": true,
  "peacock.affectPanelBorder": true,
  "peacock.affectSideBarBorder": true,
  "peacock.affectStatusAndTitleBorders": true,
  "peacock.affectTabActiveBorder": true,
  "powermode.enabled": true,
  "prettier.arrowParens": "always",
  "prettier.bracketSpacing": true,
  "prettier.embeddedLanguageFormatting": "auto",
  "prettier.endOfLine": "lf",
  "prettier.htmlWhitespaceSensitivity": "css",
  "prettier.insertPragma": false,
  "prettier.jsxBracketSameLine": false,
  "prettier.jsxSingleQuote": false,
  "prettier.printWidth": 80,
  "prettier.proseWrap": "preserve",
  "prettier.quoteProps": "as-needed",
  "prettier.requireConfig": true,
  "prettier.requirePragma": true,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "prettier.tabWidth": 2,
  "prettier.trailingComma": "none",
  "prettier.useTabs": false,
  "prettier.vueIndentScriptAndStyle": false,
  "redhat.telemetry.enabled": true,
  "remote.SSH.enableAzureIntegration": true,
  "remote.SSH.remotePlatform": {
    "Moridaira": "linux"
  },
  "remote.SSH.showLoginTerminal": true,
  "scm.inputFontFamily": "'Jetbrains Mono', Consolas",
  "search.exclude": {
    "**/*.anim": true,
    "**/bower_components": true,
    "**/logs": true,
    "**/node_modules": true,
    "**/target": true,
    "build.js": true,
    "build/": true,
    "library/": true,
    "temp/": true
  },
  "search.showLineNumbers": true,
  "security.workspace.trust.banner": "never",
  "security.workspace.trust.untrustedFiles": "open",
  "settingsSync.ignoredExtensions": [],
  "sql-formatter.uppercase": true,
  "sync.autoDownload": true,
  "sync.autoUpload": true,
  "sync.gist": "53391dc534c63c148a7d7f890f7e4334",
  "sync.quietSync": false,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.fontFamily": "'Jetbrains Mono', Consolas",
  "terminal.integrated.profiles.linux": {
    "zsh (login)": {
      "args": [
        "-l"
      ],
      "path": "zsh"
    }
  },
  "terminal.integrated.profiles.windows": {
    "Command Prompt": {
      "args": [
        "/k"
      ],
      "icon": "terminal-cmd",
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ]
    },
    "Git Bash": {
      "source": "Git Bash"
    },
    "PowerShell": {
      "args": [
        "-NoProfile",
        "-nologo"
      ],
      "icon": "terminal-powershell",
      "path": "pwsh",
      "source": "PowerShell"
    },
    "PowerShell--nologo": {
      "args": [
        "-NoProfile",
        "-nologo"
      ],
      "path": "C:\\Program Files\\PowerShell\\7\\pwsh.exe"
    }
  },
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]"
  ],
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "workbench.colorTheme": "Visual Studio Dark",
  "workbench.editor.untitled.hint": "hidden",
  "workbench.editor.wrapTabs": true,
  "workbench.editorAssociations": {
    "*.ico": "luna.editor",
    "*.jpg": "luna.editor",
    "*.png": "luna.editor"
  },
  "workbench.iconTheme": "material-icon-theme",
  "workbench.list.smoothScrolling": true,
  "workbench.productIconTheme": "fluent-icons",
  "workbench.startupEditor": "newUntitledFile"
}
