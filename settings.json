    "**/*.js": { "when": "$(basename).ts" },
    "**/.DS_Store": false,
    "**/.git": true,
    "**/.hg": false,
    "**/.svn": true,
    "**/.vs": true,
    "**/CVS": false,
    "**/node_modules": true,
    "library/": true,
    "local/": true,
    "log/": true,
    "temp/": true
  },
  "files.insertFinalNewline": true,
  "files.participants.timeout": 9999,
