    "rgba(255,255,0,0.05)",
    "rgba(0,255,0,0.05)",
    "rgba(0,255,255,0.05)",
    "rgba(0,0,255,0.05)",
    "rgba(255,0,255,0.05)"
  ],
  "less.compile": { "out": "" },
  "liveServer.settings.donotShowInfoMsg": true,
  "npm.runSilent": true,
  "powermode.combo.location": "off",
  "powermode.enabled": true,
  "powermode.shake.intensity": 1,
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "embeddedLanguageFormatting": "auto",
    "endOfLine": "lf",
    "htmlWhitespaceSensitivity": "css",
    "insertPragma": false,
    "jsxBracketSameLine": false,
    "jsxSingleQuote": false,
    "printWidth": 128,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "requireConfig": true,
    "requirePragma": true,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false,
    "vueIndentScriptAndStyle": false
  },
  "projectManager.git.baseFolders": ["H:\\"],
  "redhat.telemetry.enabled": true,
  "scm.inputFontFamily": "'Jetbrains Mono', Consolas",
  "search.exclude": {
    "**/*.anim": true,
    "**/bower_components": true,
    "**/logs": true,
