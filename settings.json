{
  "[css]": {
    "editor.defaultFormatter": "stylelint.vscode-stylelint"
  },
  "[less]": {
    "editor.defaultFormatter": "stylelint.vscode-stylelint"
  },
  "[markdown]": {
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint",
    "editor.quickSuggestions": {
      "comments": "on",
      "strings": "on",
      "other": "on"
    }
  },
  "[scss]": {
    "editor.defaultFormatter": "stylelint.vscode-stylelint"
  },
  "[sql]": {
    "editor.defaultFormatter": "adpyke.vscode-sql-formatter"
  },
  "autoimport.filesToScan": "**/*.{ts,tsx,js,jsx,vue}",
  "autoimport.useSemiColon": false,
  "bookmarks.useWorkaroundForFormatters": true,
  "cSpell.blockCheckingWhenLineLengthGreaterThan": 9999,
  "cSpell.caseSensitive": true,
  "cSpell.checkLimit": 99,
  "cSpell.diagnosticLevel": "Hint",
  "cSpell.enableFiletypes": ["!svg", "!dll", "!log", "!config"],
  "cSpell.ignorePaths": [
    "package.json",
    "package-lock.json",
    "yarn.lock",
    "extensions.json",
    "settings.json",
    "node_modules",
    ".git",
    ".gitignore",
    "*ignore",
    "*rc.json"
  ],
  "cSpell.language": "en, en-US",
  "cSpell.maxNumberOfProblems": 99,
  "cSpell.minWordLength": 4,
  "cSpell.numSuggestions": 9,
  "cSpell.showAutocompleteSuggestions": true,
  "cSpell.spellCheckOnlyWorkspaceFiles": true,
  "cSpell.suggestionNumChanges": 3,
  "cSpell.useGitignore": true,
  "cSpell.userWords": [],
  "code-runner.executorMap": {
    "c": "cd $dir && gcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "go": "go run",
    "html": "\"${env:csidlprogramfiles}\\Google\\Chrome\\Application\\chrome.exe\"",
    "java": "cd $dir && javac $fileName && java $fileNameWithoutExt",
    "javascript": "node",
    "perl": "perl",
    "php": "${env:csidlprogramfiles}\\php\\php.exe",
    "python": "python",
  },
  "code-runner.executorMapByFileExtension": { ".vbs": "cscript //Nologo" },
  "code-runner.saveFileBeforeRun": true,
  "compareFolders.ignoreEmptyLines": true,
  "compareFolders.ignoreWhiteSpaces": true,
  "debug.allowBreakpointsEverywhere": true,
  "debug.console.closeOnEnd": true,
  "debug.openExplorerOnEnd": true,
  "diffEditor.codeLens": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.maxComputationTime": 0,
  "diffEditor.maxFileSize": 99,
  "diffEditor.wordWrap": "off",
  "editor.accessibilityPageSize": 256,
  "editor.accessibilitySupport": "off",
  "editor.autoClosingBrackets": "always",
  "editor.autoClosingDelete": "always",
  "editor.autoClosingOvertype": "always",
  "editor.autoClosingQuotes": "always",
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.markdownlint": true,
    "source.fixAll.prettier": true,
    "source.fixAll.stylelint": true,
    "source.organizeImports": true
  },
  "editor.cursorSmoothCaretAnimation": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.foldingImportsByDefault": true,
  "editor.foldingMaximumRegions": 10000,
  "editor.fontFamily": "'Jetbrains Mono', Consolas",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.guides.bracketPairs": false,
  "editor.guides.indentation": false,
  "editor.hover.delay": 256,
  "editor.inlayHints.enabled": "on",
  "editor.inlineSuggest.enabled": true,
  "editor.lineNumbers": "on",
  "editor.linkedEditing": true,
  "editor.maxTokenizationLineLength": 8192,
  "editor.minimap.size": "fill",
  "editor.mouseWheelZoom": true,
  "editor.quickSuggestions": { "comments": true, "other": true, "strings": true },
  "editor.renderControlCharacters": true,
  "editor.renderLineHighlightOnlyWhenFocus": true,
  "editor.rulers": [256],
  "editor.scrollbar.verticalScrollbarSize": 5,
  "editor.smoothScrolling": true,
  "editor.suggest.localityBonus": true,
  "editor.suggest.preview": true,
  "editor.suggest.shareSuggestSelections": true,
  "editor.suggest.showStatusBar": true,
  "editor.suggestSelection": "first",
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.wordWrap": "off",
  "editor.wordWrapColumn": 80,
  "editor.wrappingIndent": "indent",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "vue-html": "html"
  },
  "errorLens.excludeBySource": ["cSpell"],
  "errorLens.fontSize": "70%",
  "errorLens.gutterIconSet": "defaultOutline",
  "errorLens.gutterIconSize": "70%",
  "errorLens.gutterIconsEnabled": true,
  "errorLens.margin": "16%",
  "errorLens.messageMaxChars": 64,
  "errorLens.messageTemplate": "$message",
  "errorLens.padding": "16px",
  "errorLens.scrollbarHackEnabled": true,
  "errorLens.severityText": ["ERROR", "WARNING", "INFO", "HINT"],
  "errorLens.statusBarColorsEnabled": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.associations": { "*.svg": "html" },
  "files.autoGuessEncoding": true,
  "files.autoSave": "onFocusChange",
  "files.autoSaveDelay": 999,
  "files.eol": "\n",
  "files.exclude": {
    "**/*.js": { "when": "$(basename).ts" },
    "**/.DS_Store": false,
    "**/.git": true,
    "**/.hg": false,
    "**/.svn": true,
    "**/.vs": true,
    "**/CVS": false,
    "**/node_modules": true,
    "library/": true,
    "local/": true,
    "log/": true,
    "temp/": true
  },
  "files.insertFinalNewline": true,
  "files.participants.timeout": 9999,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.ignoreLegacyWarning": true,
  "git.suggestSmartCommit": true,
  "gitlens.advanced.messages": { "suppressGitMissingWarning": true },
  "gitlens.codeLens.includeSingleLineSymbols": true,
  "guides.active.color.dark": "rgba(102,204,255,0.75)",
  "i18n-ally.enabledParsers": ["ts", "js", "json"],
  "indentRainbow.colors": [
    "rgba(255,0,0,0.05)",
    "rgba(255,165,0,0.05)",
    "rgba(255,255,0,0.05)",
    "rgba(0,255,0,0.05)",
    "rgba(0,255,255,0.05)",
    "rgba(0,0,255,0.05)",
    "rgba(255,0,255,0.05)"
  ],
  "javascript.format.insertSpaceAfterConstructor": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingTemplateStringBraces": true,
  "javascript.format.insertSpaceBeforeFunctionParenthesis": true,
  "javascript.format.semicolons": "remove",
  "javascript.inlayHints.enumMemberValues.enabled": true,
  "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
  "javascript.inlayHints.parameterNames.enabled": "all",
  "javascript.inlayHints.parameterTypes.enabled": true,
  "javascript.inlayHints.propertyDeclarationTypes.enabled": true,
  "javascript.inlayHints.variableTypes.enabled": true,
  "javascript.preferences.importModuleSpecifierEnding": "minimal",
  "javascript.referencesCodeLens.enabled": true,
  "javascript.referencesCodeLens.showOnAllFunctions": true,
  "javascript.suggest.completeFunctionCalls": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "js/ts.implicitProjectConfig.checkJs": true,
  "npm.runSilent": true,
  "powermode.combo.location": "off",
  "powermode.enabled": true,
  "powermode.shake.intensity": 1,
  "prettier.arrowParens": "always",
  "prettier.bracketSpacing": true,
  "prettier.embeddedLanguageFormatting": "auto",
  "prettier.endOfLine": "lf",
  "prettier.htmlWhitespaceSensitivity": "css",
  "prettier.insertPragma": false,
  "prettier.bracketSameLine": false,
  "prettier.jsxSingleQuote": false,
  "prettier.printWidth": 128,
  "prettier.proseWrap": "preserve",
  "prettier.quoteProps": "as-needed",
  "prettier.requireConfig": true,
  "prettier.requirePragma": true,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "prettier.tabWidth": 2,
  "prettier.trailingComma": "none",
  "prettier.useTabs": false,
  "prettier.vueIndentScriptAndStyle": false,
  "projectManager.git.baseFolders": ["H:\\"],
  "redhat.telemetry.enabled": true,
  "scm.inputFontFamily": "'Jetbrains Mono', Consolas",
  "search.exclude": {
    "**/*.anim": true,
    "**/bower_components": true,
    "**/logs": true,
    "**/node_modules": true,
    "**/target": true,
    "build.js": true,
    "build/": true,
    "library/": true,
    "temp/": true
  },
  "search.showLineNumbers": true,
  "security.workspace.trust.banner": "never",
  "security.workspace.trust.untrustedFiles": "open",
  "settingsSync.ignoredExtensions": [],
  "sortLines.sortEntireFile": true,
  "sql-formatter.uppercase": true,
  "sync.autoDownload": true,
  "sync.autoUpload": true,
  "sync.gist": "53391dc534c63c148a7d7f890f7e4334",
  "sync.quietSync": false,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.enablePersistentSessions": false,
  "terminal.integrated.fontFamily": "'Jetbrains Mono', Consolas",
  "terminal.integrated.profiles.linux": { "zsh (login)": { "args": ["-l"], "path": "zsh" } },
  "terminal.integrated.profiles.windows": {
    "Command Prompt": {
      "args": ["/k"],
      "icon": "terminal-cmd",
      "path": ["${env:windir}\\Sysnative\\cmd.exe", "${env:windir}\\System32\\cmd.exe"]
    },
    "Git Bash": { "source": "Git Bash" },
    "PowerShell": {
      "args": ["-NoProfile", "-Nologo"],
      "icon": "terminal-powershell",
      "path": "pwsh",
      "source": "PowerShell"
    },
    "PowerShell--nologo": {
      "args": ["-NoProfile", "-Nologo"],
      "path": "${env:csidlprogramfiles}\\PowerShell\\7\\pwsh.exe"
    }
  },
  "todo-tree.general.enableFileWatcher": true,
  "todo-tree.general.tags": ["BUG", "HACK", "FIXME", "TODO", "XXX", "[ ]", "[x]"],
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "typescript.format.insertSpaceAfterConstructor": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingTemplateStringBraces": true,
  "typescript.format.insertSpaceAfterTypeAssertion": true,
  "typescript.format.insertSpaceBeforeFunctionParenthesis": true,
  "typescript.implementationsCodeLens.enabled": true,
  "typescript.inlayHints.enumMemberValues.enabled": true,
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.inlayHints.parameterTypes.enabled": true,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
  "typescript.inlayHints.variableTypes.enabled": true,
  "typescript.referencesCodeLens.enabled": true,
  "typescript.referencesCodeLens.showOnAllFunctions": true,
  "vetur.validation.script": false,
  "workbench.colorCustomizations": {
    "editor.lineHighlightBackground": "#1073cf2d",
    "editor.lineHighlightBorder": "#9fced11f"
  },
  "workbench.commandPalette.history": 32,
  "workbench.commandPalette.preserveInput": true,
  "workbench.editor.historyBasedLanguageDetection": true,
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.value": 16,
  "workbench.editor.untitled.hint": "hidden",
  "workbench.editor.wrapTabs": true,
  "workbench.editorAssociations": {
    "*.ico": "luna.editor",
    "*.jpg": "luna.editor",
    "*.png": "luna.editor"
  },
  "workbench.iconTheme": "material-icon-theme",
  "workbench.list.smoothScrolling": true,
  "workbench.productIconTheme": "fluent-icons",
  "workbench.quickOpen.preserveInput": true,
  "workbench.startupEditor": "newUntitledFile"
}
