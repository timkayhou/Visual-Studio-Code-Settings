{
  "[css]": {
    "editor.defaultFormatter": "stylelint.vscode-stylelint"
  },
  "[less]": {
    "editor.defaultFormatter": "stylelint.vscode-stylelint"
  },
  "[markdown]": {
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
  },
  "[scss]": {
    "editor.defaultFormatter": "stylelint.vscode-stylelint"
  },
  "[sql]": {
    "editor.defaultFormatter": "adpyke.vscode-sql-formatter"
  },
  "bookmarks.useWorkaroundForFormatters": true,
  "cSpell.caseSensitive": true,
  "cSpell.enableFiletypes": ["!svg", "!dll", "!log", "!config"],
  "cSpell.ignorePaths": [
    "package.json",
    "package-lock.json",
    "yarn.lock",
    "extensions.json",
    "settings.json",
    "node_modules",
    ".git",
    ".gitignore",
    "*ignore",
    "*rc.json"
  ],
  "cSpell.language": "en, en-US",
  "cSpell.maxNumberOfProblems": 99,
  "cSpell.minWordLength": 4,
  "cSpell.numSuggestions": 9,
  "cSpell.spellCheckOnlyWorkspaceFiles": true,
  "cSpell.suggestionNumChanges": 3,
  "cSpell.useGitignore": true,
  "cSpell.userWords": [],
  "debug.allowBreakpointsEverywhere": true,
  "debug.console.closeOnEnd": true,
  "debug.openExplorerOnEnd": true,
  "diffEditor.codeLens": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.maxComputationTime": 0,
  "editor.accessibilityPageSize": 256,
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.markdownlint": true,
    "source.fixAll.prettier": true,
    "source.fixAll.stylelint": true,
    "source.organizeImports": true
  },
  "editor.codeLensFontFamily": "'Jetbrains Mono', Consolas",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.foldingImportsByDefault": true,
  "editor.fontFamily": "'Jetbrains Mono', Consolas",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.inlineSuggest.enabled": true,
  "editor.lineNumbers": "on",
  "editor.linkedEditing": true,
  "editor.maxTokenizationLineLength": 10000,
  "editor.minimap.size": "fill",
  "editor.mouseWheelZoom": true,
  "editor.renderControlCharacters": true,
  "editor.renderLineHighlightOnlyWhenFocus": true,
  "editor.rulers": [256],
  "editor.scrollbar.verticalScrollbarSize": 5,
  "editor.smoothScrolling": true,
  "editor.suggest.shareSuggestSelections": true,
  "editor.suggest.showStatusBar": true,
  "editor.suggestSelection": "first",
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 80,
  "editor.wrappingIndent": "indent",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "vue-html": "html"
  },
  "errorLens.excludeBySource": ["cSpell"],
  "errorLens.fontSize": "70%",
  "errorLens.gutterIconSet": "defaultOutline",
  "errorLens.gutterIconSize": "70%",
  "errorLens.gutterIconsEnabled": true,
  "errorLens.margin": "16%",
  "errorLens.messageMaxChars": 64,
  "errorLens.messageTemplate": "$message",
  "errorLens.padding": "16px",
  "errorLens.scrollbarHackEnabled": true,
  "errorLens.severityText": ["ERROR", "WARNING", "INFO", "HINT"],
  "errorLens.statusBarColorsEnabled": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.associations": {
    "*.js": "javascript",
    "*.jsx": "javascriptreact",
    "*.ts": "typescript",
    "*.tsx": "typescriptreact",
    "*.vue": "vue"
  },
  "files.autoGuessEncoding": true,
  "files.autoSave": "onFocusChange",
  "files.eol": "\n",
  "files.exclude": {
    "**/*.js": {"when": "$(basename).ts"},
    "**/.DS_Store": false,
    "**/.git": true,
    "**/.hg": false,
    "**/.svn": true,
    "**/.vs": true,
    "**/CVS": false,
    "**/Thumbs.db": false,
    "**/node_modules": true,
    "library/": true,
    "local/": true,
    "temp/": true
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.ignoreLegacyWarning": true,
  "git.suggestSmartCommit": true,
  "gitlens.advanced.messages": {"suppressGitMissingWarning": true},
  "gitlens.codeLens.includeSingleLineSymbols": true,
  "guides.active.color.dark": "rgba(102,204,255,0.75)",
  "i18n-ally.enabledParsers": ["ts", "js", "json"],
  "indentRainbow.colors": [
    "rgba(255,0,0,0.05)",
    "rgba(255,165,0,0.05)",
    "rgba(255,255,0,0.05)",
    "rgba(0,255,0,0.05)",
    "rgba(0,255,255,0.05)",
    "rgba(0,0,255,0.05)",
    "rgba(255,0,255,0.05)"
  ],
  "javascript.validate.enable": false,
  "less.compile": {"out": ""},
  "liveServer.settings.donotShowInfoMsg": true,
  "npm.runSilent": true,
  "powermode.combo.location": "off",
  "powermode.enabled": true,
  "powermode.shake.intensity": 1,
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "embeddedLanguageFormatting": "auto",
    "endOfLine": "lf",
    "htmlWhitespaceSensitivity": "css",
    "insertPragma": false,
    "jsxBracketSameLine": false,
    "jsxSingleQuote": false,
    "printWidth": 128,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "requireConfig": true,
    "requirePragma": true,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false,
    "vueIndentScriptAndStyle": false
  },
  "projectManager.git.baseFolders": ["H:\\"],
  "redhat.telemetry.enabled": true,
  "scm.inputFontFamily": "'Jetbrains Mono', Consolas",
  "search.exclude": {
    "**/*.anim": true,
    "**/bower_components": true,
    "**/logs": true,
    "**/node_modules": true,
    "**/target": true,
    "build.js": true,
    "build/": true,
    "library/": true,
    "temp/": true
  },
  "search.showLineNumbers": true,
  "security.workspace.trust.banner": "never",
  "security.workspace.trust.untrustedFiles": "open",
  "settingsSync.ignoredExtensions": [],
  "sql-formatter.uppercase": true,
  "sync.autoDownload": true,
  "sync.autoUpload": true,
  "sync.gist": "53391dc534c63c148a7d7f890f7e4334",
  "sync.quietSync": false,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.fontFamily": "'Jetbrains Mono', Consolas",
  "terminal.integrated.profiles.linux": {"zsh (login)": {"args": ["-l"], "path": "zsh"}},
  "terminal.integrated.profiles.windows": {
    "Command Prompt": {
      "args": ["/k"],
      "icon": "terminal-cmd",
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ]
    },
    "Git Bash": {"source": "Git Bash"},
    "PowerShell": {
      "args": ["-NoProfile", "-nologo"],
      "icon": "terminal-powershell",
      "path": "pwsh",
      "source": "PowerShell"
    },
    "PowerShell--nologo": {
      "args": ["-NoProfile", "-nologo"],
      "path": "C:\\Program Files\\PowerShell\\7\\pwsh.exe"
    }
  },
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]"
  ],
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "workbench.colorTheme": "Visual Studio Dark",
  "workbench.editor.untitled.hint": "hidden",
  "workbench.editor.wrapTabs": true,
  "workbench.editorAssociations": {
    "*.ico": "luna.editor",
    "*.jpg": "luna.editor",
    "*.png": "luna.editor"
  },
  "workbench.iconTheme": "material-icon-theme",
  "workbench.list.smoothScrolling": true,
  "workbench.productIconTheme": "fluent-icons",
  "workbench.startupEditor": "newUntitledFile"
}
